{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["babel", "react", "jsx-a11y", "import", "jest"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "flowtype/define-flow-type": 0
  }
}
